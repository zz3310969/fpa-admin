{
  "entry": "src/index.js",
  "define": {
    "testWsUrl": "wss://minipro.fpaworld.com/wss/roof-im/connect.ws",
    "productionWsUrl": "wss://minipro.fpaworld.com/wss/roof-im/connect.ws"
  },
  "extraBabelPlugins": [
    "transform-runtime",
    "transform-decorators-legacy",
    "transform-class-properties",
    ["import", { "libraryName": "antd", "libraryDirectory": "es", "style": true }]
  ],
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr"
      ]
    }
  },
  "externals": {
    "g2": "G2",
    "g-cloud": "Cloud",
    "g2-plugin-slider": "G2.Plugin.slider"
  },
  "ignoreMomentLocale": true,
  "theme": "./src/theme.js",
  "hash":true,
  "proxy": {
    "/api": {
      "target": "http://localhost:8081/fpa-web",
      "ws": true,
      "pathRewrite": {
        "/api": "/"
      }
    }
  }

}
